{
	"name": "ethtps.frontend.v2",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@babel/core": ">=7.0.0-0 <8.0.0",
		"@babel/plugin-syntax-flow": "^7.14.5",
		"@babel/plugin-transform-react-jsx": "^7.14.9",
		"@react-spring/web": "^9.6.1",
		"@types/jest": "^27.5.2",
		"@types/node": "^18.14.0",
		"@types/url-parse": "^1.4.8",
		"date-fns": "^2.29.3",
		"env-cmd": "^10.1.0",
		"ethtps.api.client": "../ethtps.api.client",
		"ethtps.components": "../ethtps.components",
		"ethtps.components.utils": "../ethtps.components.utils",
		"ethtps.data": "../ethtps.data",
		"groupby-js": "^1.1.9",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"moment": "^2.29.4",
		"node-sass": "^8.0.0",
		"prop-types": "^15.8.1",
		"query-string": "^8.1.0",
		"react": "^18.2.0",
		"react-device-detect": "^2.2.2",
		"react-dom": "^18.2.0",
		"react-markdown": "^8.0.5",
		"react-query": "^3.39.3",
		"react-router": "^6.8.0",
		"react-router-dom": "^6.8.0",
		"react-scripts": "5.0.1",
		"react-tap-event-plugin": "^3.0.3",
		"react-transition-group": "^4.4.5",
		"relative-deps": "^1.0.7",
		"sort-by": "^1.2.0",
		"ts-extras": "^0.11.0",
		"typescript": "^4.9.4",
		"web-vitals": "^2.1.4",
		"webpack": "^5.75.0"
	},
	"scripts": {
		"start": "set PORT=3007 && react-scripts start",
		"start:dev": "set PORT=3007 && env-cmd -f dev.env npm start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public",
		"prepare": "relative-deps",
		"lint": "npx eslint src test",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "npx prettier src test --check",
		"prettier:fix": "npm run prettier -- --write"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"babel-plugin-named-exports-order": "^0.0.2",
		"eslint": "^8.0.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.32.2",
		"prettier": "2.8.4"
	},
	"relativeDependencies": {
		"ethtps.data": "../ethtps.data",
		"ethtps.api.client": "../ethtps.api.client",
		"ethtps.components": "../ethtps.components"
	}
}
